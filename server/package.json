{
  "name": "caretaker-server",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/game_server/ & node dist/login_server/",
    "start-login-server": "node dist/login_server/",
    "start-game-server": "node dist/game_server/",
    "start-ls": "node dist/login_server/",
    "start-gs": "node dist/game_server/",
    "build": "tsc",
    "watch-tsc": "tsc --watch",
    "watch-start": "nodemon dist/game_server/ & nodemon dist/login_server/",
    "watch-start-login-server": "nodemon dist/login_server/",
    "watch-start-game-server": "nodemon dist/game_server/",
    "watch-start-ls": "tsc --watch & nodemon dist/login_server/",
    "watch-start-gs": "tsc --watch & nodemon dist/game_server/",
    "test-client": "node dist/test_client/",
    "dev": "npm run watch-tsc & npm run watch-start & npm run test-client",
    "start-deamon": "forever start dist/login_server/index.js & forever start dist/game_server/index.js",
    "list-deamon": "forever list",
    "stop-deamon": "kill",
    "test": "tsc && mocha dist/**/*.spec.js",
    "lint": "eslint src --ext ts",
    "tsc": "node_modules/.bin/tsc",
    "nodemon": "node_modules/.bin/nodemon",
    "mocha": "node_modules/.bin/mocha",
    "eslint": "node_modules/.bin/eslint",
    "forever": "node_modules/.bin/forever"
  },
